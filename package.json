{
  "private": true,
  "name": "cloudflare-worker-template",
  "version": "1.1.0",
  "description": "A template for kick starting a Cloudflare Workers project",
  "main": "dist/worker.js",
  "scripts": {
    "dev": "wrangler dev",
    "publish": "wrangler publish --env ${NODE_ENV}",
    "test": "jest --config jestconfig.json --verbose",
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "semantic-release": "semantic-release",
    "initialize-repo": "husky install"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "author": "Markus Ahlstrand <markus@sesamy.com>",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^28.1.6",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@sagi.io/workers-jwt": "^0.0.23",
    "cloudworker-router": "3.0.0",
    "serverless-cloudflare-workers": "^1.2.0"
  }
}
