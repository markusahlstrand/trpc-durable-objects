{
  "private": true,
  "name": "trpc-durable-objects",
  "version": "1.1.0",
  "description": "Testing out trpc for durable objects",
  "main": "dist/worker.js",
  "scripts": {
    "dev": "wrangler dev",
    "publish": "wrangler publish --env ${NODE_ENV}",
    "test": "jest --config jestconfig.json --verbose",
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "semantic-release": "semantic-release",
    "initialize-repo": "husky install"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "author": "Markus Ahlstrand <markus@sesamy.com>",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/prompt-cli": "^17.4.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.4.0",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "semantic-release": "^20.1.0",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "wrangler": "^2.8.1"
  },
  "dependencies": {
    "@sagi.io/workers-jwt": "^0.0.23",
    "@trpc/client": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "cloudworker-router": "4.1.0",
    "serverless-cloudflare-workers": "^1.2.0",
    "zod": "^3.0.0"
  }
}
